<!--Section: Contact v.2-->
<section class="mb-4">

    <!--Section heading-->
    <div class="text-center">
        <h2 class="h1-responsive font-weight-bold text-center my-4 mt-5">Contact us</h2>
        <p class="mb-4 text-muted">Feel free to send us a message</p>
    </div>
    <!--Section description-->
    <!-- Bootstrap Icon -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
<div class="container-fluid px-5 my-5">
    <div class="row justify-content-center">
      <div class="col-xl-10">
 
            <!--Grid column-->
            <div class="col-md-12 mb-md-0 mb-5 mt-3">
                <form name="contact-form" class="contact-form">
                    <!--Grid row-->
                    <div class="row">
                        <!--Grid column-->
                        <div class="col-md-6">
                            <div class="md-form mb-3">
                                <input type="text" id="cname" name="cname" class="form-control" placeholder="Name">
                                
                            </div>
                        </div>
                        <!--Grid column-->
                        <!--Grid column-->
                        <div class="col-md-6">
                            <div class="md-form mb-3">
                                <input type="text" id="cemail" name="cemail" class="form-control"placeholder="Email">
                                
                            </div>
                        </div>
                        <!--Grid column-->
                    </div>
                    <!--Grid row-->
                    <!--Grid row-->
                    <div class="row">
                        <div class="col-md-12">
                            <div class="md-form mb-4">
                                <input type="text" id="subject" name="subject" class="form-control" placeholder="Subject">
                            </div>
                        </div>
                    </div>
                    <!--Grid row-->
                    <!--Grid row-->
                    <div class="row">
                        <!--Grid column-->
                        <div class="col-md-12">
                            <div class="md-form mb-3">
                                <textarea type="text" id="message" name="message" rows="2" class="form-control md-textarea" placeholder="Text Message"></textarea>
                            </div>
                        </div>
                    </div>
                    <!--Grid row-->
                    <div class="text-center text-md-left">
                        <!-- <a class="btn btn-primary" onclick="document.getElementById('contact-form').submit();">Send</a> -->
                        <input type="submit" class="submit btn btn-primary" value ="Send Message">
                    </div>
                    <div class="status"></div>
                </form>         
            </div>
        </div>


        </div>
    </div>
</div>
</section>

